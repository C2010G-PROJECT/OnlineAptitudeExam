@{
    Layout = null;
}



<div class="card card-body shadow-sm">
    <table class="table table-custom table-hover table-bordered">
        <thead>
            <tr>
                <th class="sortable">
                    #
                </th>
                <th style="width:25%" class="sortable">
                    Exam Name
                </th>
                <th class="sortable">
                    Number Of Uses
                </th>
            </tr>
        </thead>

        <tbody>
            @for (int i = 0; i < 10; i++)
            {

                <tr @*data-id='@item.id'*@>
                    <td>
                        @*@((Model.PageNumber - 1) * Model.PageSize + i + 1)*@
                    </td>
                    <td class="field-name">
                        aaa
                    </td>

                </tr>
            }
        </tbody>
    </table>
    <br />

</div>


